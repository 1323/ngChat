{
  "name": "ng-chat",
  "version": "0.0.1",
  "description": "AngularJS & TypeScript & webpack",
  "main": "webpack.config.js",
  "scripts": {
    "postinstall": "typings install",
    "dev": "webpack-dev-server --config config/webpack.dev.js",
    "build": "webpack --progress --colors --display-error-details --display-reasons -p --config config/webpack.build.js",
    "karma": "node_modules/.bin/karma start config/karma.config.js",
    "test": "npm run karma",
    "tdd": "npm run karma -- --auto-watch --no-single-run --colors",
    "tdd:full": "npm run tdd -- --browsers Chrome,Firefox,PhantomJS",
    "tdd:full:osx": "npm run tdd -- --browsers Chrome,Firefox,PhantomJS,Safari"
  },
  "keywords": [
    "angular",
    "ES6",
    "webpack",
    "javascript",
    "chat"
  ],
  "author": "Jacek Pudysz <jacekpudysz@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "angular": "1.5.7",
    "angular-animate": "1.5.7",
    "angular-aria": "1.5.7",
    "angular-material": "1.1.0-rc.5",
    "angular-ui-router": "0.3.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.10",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "glob-all": "3.0.3",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "jasmine-core": "2.4.1",
    "json-loader": "0.5.4",
    "karma": "1.1.1",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.0.4",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-typescript-preprocessor2": "1.2.1",
    "karma-webpack": "1.7.0",
    "ng-annotate-loader": "0.1.0",
    "node-sass": "3.8.0",
    "open-browser-webpack-plugin": "0.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "sass": "0.5.0",
    "sass-loader": "4.0.0",
    "ts-loader": "0.8.2",
    "tslint": "3.13.0",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "typings": "1.3.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.14.0"
  }
}
